<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "#", id: "logo" do %>
      <%= t ".name" %>
    <% end %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li>
          <%= link_to root_path do %>
            <%= t "static_pages.home.home_title" %>
          <% end %>
        </li>
        <li>
          <%= link_to help_path do %>
            <%= t "static_pages.help.help_title" %>
          <% end %>
        </li>
        <li>
          <%= link_to "#" do %>
            <%= t ".login" %>
          <% end %>
        </li>
        <li>
          <%= link_to locale:"en" do %>
            <%= t "english" %>
          <% end %>
        </li>
        <li>
          <%= link_to locale:"vi"  do %>
            <%= t "vietnam" %>
          <% end %>
        </li>
      </ul>
    </nav>
  </div>
</header>
